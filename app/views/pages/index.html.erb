<h1>Pages#index</h1>
<p>Find me in app/views/pages/index.html.erb</p>
<%= link_to 'New Page', new_page_path %>
<% @pages.each do |page| %>
<%= page.title %>
  <%= link_to 'Show Page', page_path(page) %>
  <%= link_to 'Edit Page', edit_page_path(page) %>
  <%= link_to 'Destroy Page', page, method: :delete %>
<% end %>



